  "dependencies": {
    "aphrodite": "latest",
    "axios": "^0.18.0",
    "create-react-class": "^15.6.3",
    "history": "latest",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-loading": "latest",
    "react-redux": "latest",
    "react-route": "^1.0.3",
    "react-router": "latest",
    "react-router-dom": "latest",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.1.0",
    "redux": "latest",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build-rename",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-rename": "npm run build-rename-js && npm run build-rename-css",
    "build-rename-js": "renamer --regex --find \"main\\.[^\\.]+\\.js\" --replace sd_total.js \"build/static/js/*.js\"",
    "build-rename-css": "renamer --regex --find \"main\\.[^\\.]+\\.css\" --replace sd_total.css \"build/static/css/*.css\"",
    "build-rename-map": "renamer -v --regex --find \"main\\.[^\\.]+\\.map\" --replace sd_total.map \"build/static/**/*.map\""
  },
  "devDependencies": {
    "renamer": "^0.7.3"
  }
}
